<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="900.0" stylesheets="@tableview.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.second.tetris.controller.ScoreBoardController">
   <children>
      <TableView fx:id="tableview" layoutX="180.0" layoutY="23.0" prefHeight="564.0" prefWidth="900.0" tableMenuButtonVisible="true">
        <columns>
          <TableColumn fx:id="score" prefWidth="75.0" text="Score" />
          <TableColumn fx:id="date" prefWidth="75.0" text="Date" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
